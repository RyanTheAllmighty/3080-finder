{
    "compilerOptions": {
        "lib": [
            "es2020",
            "dom"
        ],
        "module": "commonjs",
        "target": "es2019",
        "outDir": "dist",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "alwaysStrict": true,
        "strict": true,
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "declaration": false,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}
